{
  "name": "mui-theme-generator",
  "version": "1.0.0",
  "description": "Material-UI theme/palette generator",
  "repository": {
    "type": "git",
    "url": "https://github.com/cimdalli/mui-theme-generator.git"
  },
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "perf": "cross-env NODE_ENV=production webpack-dev-server --config webpack-dev-server.config.js --progress --inline --colors",
    "build": "webpack --config webpack-production.config.js --progress --colors"
  },
  "private": true,
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "cross-env": "^3.2.4",
    "html-webpack-plugin": "^2.24.1",
    "react-hot-loader": "next",
    "transfer-webpack-plugin": "^0.1.4",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "copy-to-clipboard": "^3.0.5",
    "file-saver": "^1.3.3",
    "material-ui": "^0.17.1",
    "react": "^15.4.1",
    "react-addons-perf": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-tap-event-plugin": "^2.0.1"
  }
}
